#include <stdio.h>
#include <string.h>

int main()
{
    int n;
    int arr[n];
    
    int i;
    // scanf("%d",&arr[i]);
    printf("enter how many elemnts");
    scanf("%d",&n);
    
    printf("enter elemnts:");
    
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);  // Read each element using its address
    }
    int sum=0;
    for(int i=0;i<n;i++){
        sum=sum+arr[i];
    }
    printf("%d",sum);
    return 0;
    
}
