#include <stdio.h>
nt main() {
    int n;
    int arr[30]; // Assuming a maximum of 30 elements (adjust as needed)

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    
    printf("Enter %d elements:\n", n);

    // Read elements from the user
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Calculate and print the sum
    int sum = 0;
    for (int i = 0; i < n; i++) {
        sum += arr[i];
    }
    printf("Sum of elements: %d\n", sum);

    return 0;
}

